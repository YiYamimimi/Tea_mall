/* 第一部分 */

.view_1 {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 400rpx;
}

.background-image{
  height: 400rpx;
   position: absolute;
   width: 100%;
   left: 0px;
   top: 0px;
   border-radius: 50rpx;
}
.root {
  width: 100%;
   position: relative;
}

.content{
  position: absolute;
  width: 100%;
  left: 0px;
  top: 0px;
}

.view_image_text {
  width: 100%;
  display: flex;
  align-items: left;
  justify-content: left;
  margin-top: 150rpx;
  margin-left: 50rpx;
  flex-direction: row;
}
.view_image_text text{
    margin-top: 30rpx;
}

.image_radius {
  height: 60px;
  width: 60px;
  margin-right: 40rpx;
  border-radius: 40px;
}
/* 第二部分 */

.view_2 {
  width: 100%;
  height: 150rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  background: white;
  margin: 10rpx;
}

.view_2 .myorder{
  font-size: 35rpx;
  height: 100rpx;
  position: relative;
  left: -290rpx;
  line-height: 80rpx;
  margin-top: 8rpx;
}
.myorders{
    margin-top: 1rpx;
}
.view_tupianwenzi {
  display: flex;
  flex-direction: column;
  width: 25%;
  align-items: center;
}

.image_tupian {
  display: flex;
  width: 100rpx;
  height: 100rpx;
}
