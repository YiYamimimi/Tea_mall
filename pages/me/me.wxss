/* 第三部分 */

.view_3 {
  width: 100%;
  height: 75%;
  margin: 10rpx;
  /* background: #f0eeed; */
}

.list-item {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 70rpx;
  margin-top: 20rpx;
  position: relative; /*父元素位置要设置为相对*/
  background: white;
}
/* 收货地址管理-关于我们 */
.list-item2{
  height: 367rpx;
}

.item-image {
  width:70rpx;
  height: 70rpx;
}

.icon_image{
  width: 50rpx;
  height: 50rpx;
  margin-left: 20rpx;
}

.item_text_image{
  flex-direction: column;
}

.item-text {
  color: gray;
  font-size: 35rpx;
  margin-left: 20rpx;
  position: relative;
  bottom: 22rpx;
}

.image-jiantou {
  width: 20rpx;
  height: 35rpx;
  position: absolute; /* 要约束所在位置的子元素的位置要设置成绝对 */
  right: 0; /* 靠右调节 */
  margin-right: 35rpx;
}

/* 黑线 使得产生阴影效果 */
.line {
  width: 100%;
  height: 3rpx;
  background: lightgray;
  margin-left: 90rpx;
}

.text_saoyisao{
  font-size: 35rpx;
  /* 与图片之间的间隔 */
  margin-top: 5rpx;
}

.loding{
  flex-direction: column;
  flex-direction: row;
  margin: 800rpx;
 background-color: #009e4f;
